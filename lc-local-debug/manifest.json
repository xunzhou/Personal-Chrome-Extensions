{
  "name": "LeetCode Local Debug",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Generate Local file for LeetCode debugging.",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_ui":{
    "page":"options.html"
  },
  "background": {
    "scripts": ["options.js"]
  },
  "permissions": ["tabs", "<all_urls>", "clipboardWrite", "storage"] 
}