{
    "manifest_version": 2,
    "name": "Clean New Tab Page",
    "short_name": "Blank New Tab",
    "version": "1.0.5",
    "minimum_chrome_version": "29",
    "description": "Replaces the New Tab page with a blank one.",
    "icons": {
        "16": "16.png",
        "32": "32.png",
        "48": "48.png",
        "128": "128.png"
    },
    "chrome_url_overrides": {
        "newtab": "n.html"
    },
    "offline_enabled": true
}